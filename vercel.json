{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        }
    ],
    "builds": [
        {
            "src": "angular.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist/project-angular"
            }
        },
        {
            "src": "api/**/*.ts",
            "use": "@vercel/node"
        }
    ]
}